# Browser-Use API Service Configuration

# LLM API Keys (at least one required)
# ‚≠ê RECOMMENDED: Google Gemini (fastest, cheapest, 60 req/min free tier)
# Get your key: https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Alternative LLM providers (optional)
BROWSER_USE_API_KEY=your_browser_use_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
PORT=8765
ENVIRONMENT=production
LOG_LEVEL=INFO

# Browser Configuration
MAX_CONCURRENT_BROWSERS=2
BROWSER_TIMEOUT=300
MAX_STEPS=40

# Browser Display Mode
# Set to false to see browser window (for local debugging)
# Set to true for production/Railway (no display available)
# Note: Will be forced to true in Docker/Railway environments
HEADLESS_BROWSER=true

# Rate Limiting
RATE_LIMIT_REQUESTS=10
RATE_LIMIT_WINDOW=60

# Optional Settings
ANONYMIZED_TELEMETRY=false
CORS_ORIGINS=["*"]

# Docker/Railway specific (auto-detected)
IN_DOCKER=false